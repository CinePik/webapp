{
  "name": "webapp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.dev.json -c development",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "prettier": "npx prettier --write .",
    "generate:api:catalog": "openapi-generator-cli generate -i http://localhost:3001/api-json -g typescript-angular -o src/app/api/catalog --additional-properties=withInterfaces=true",
    "generate:api:watchlist": "openapi-generator-cli generate -i http://localhost:3002/api-json -g typescript-angular -o src/app/api/watchlist --additional-properties=withInterfaces=true",
    "generate:api:recommendation": "openapi-generator-cli generate -i http://localhost:3003/api-json -g typescript-angular -o src/app/api/recommendation-engine --additional-properties=withInterfaces=true"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0-rc.2",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.3",
    "keycloak-angular": "^15.0.0",
    "keycloak-js": "^22.0.5",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.10",
    "@angular-eslint/builder": "17.2.0",
    "@angular-eslint/eslint-plugin": "17.2.0",
    "@angular-eslint/eslint-plugin-template": "17.2.0",
    "@angular-eslint/schematics": "17.2.0",
    "@angular-eslint/template-parser": "17.2.0",
    "@angular/cli": "^17.0.10",
    "@angular/compiler-cli": "^17.0.0",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "6.18.0",
    "@typescript-eslint/parser": "6.18.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.2.2",
    "prettier-eslint": "^16.2.0",
    "typescript": "~5.2.2"
  }
}
